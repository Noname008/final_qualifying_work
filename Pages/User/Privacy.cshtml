@page
@model PrivacyModel
@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<h2>Список пользователей</h2>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Имя пользователя</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model.Users)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.Name</td>
            </tr>
        }
    </tbody>
</table>

@if (!Model.Users.Any())
{
    <p>Пользователи не найдены.</p>
}
